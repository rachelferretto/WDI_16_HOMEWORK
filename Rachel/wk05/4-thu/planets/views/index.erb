<h2>Current Planets</h2>



<button>
<a href="/planets/new">Create a Planet</a>
</button>




<div class= "wrapper">
    <div class="planets">

    <% @planets.each do |planet| %>

        <a href="/planets/<%= planet.id %>">
            <%= planet.name %> <br>
        <img class ="thumbnail" src="<%= planet.img_url %>">
        <br>
    <% end %>
    </div>
    
</div>